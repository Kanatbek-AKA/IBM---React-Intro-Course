.App-body {
  background-color: black;
  background-image: none;
  font: 100% system-ui;
}

.boo {
 width: 100%;
 height: 100px;   
}

.foo {
  align-items: center;
  justify-content: center;
  
}

.nav-main {
    display: inline-flex;
}

.navigator {
  position:absolute;
  top: 50px;
  right: 50px;
}

.link {
    color: rgb(212,175,55);
    font-size: 20px;
    font-style: normal;
    margin-left: 25px;
    text-decoration: none;
}

.link:active {
    color: rgb(212,175,55);
}

.link:hover,
.link:focus {
    color: lightblue;
    cursor: pointer;
}

.link:visited {
    color: ghostwhite;
    opacity: 0.7;
    text-decoration: none;
}

.dateTime {
  position: absolute;
  top: 70px;
  left: 60px; 
  color: snow; 
  font-weight: bold;
}

.logo {
 width: 100px; 
 height: 60px;
 position: absolute;
 top: 10px;
 left: 90px;
 filter: brightness(1.2) contrast(1.8);
}

.main {
  margin-top: 150px;
  margin-left: 250px;
  margin-right: 250px;
  font-family: "Times New Roman", Helvetica, Arial, Verdana, sans-serif;
  text-align: center;
  justify-content: center;
  background: white;
}

.h1 {
  font-size: 50px;
  color: darkgreen;
  text-shadow: 2px 2px 2px lightblue;
}

p {
    font-size: 20px;
    letter-spacing: 1.2px;
    margin-top: 50px;
}

.text {
    font-family: "Times New Romane", sans-serif;
    font-size: 18px;
    text-align: left;
    margin-top: 100px;
    padding: 15px;
    border: thin solid hidden;
    width: 1000px;
    text-overflow: ellipsis-word;
    word-break: break-word;
}

.list {
    display: list-item;
    font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-size: 15px;
    padding: 5px;
    margin-left: 30px;
}

.todoList {
    width: 500px;
    margin: 60px auto;
}

.h2 {
  font-size: 25px;
  padding: 10px;
}



